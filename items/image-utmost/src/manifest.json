{
  "manifest_version": 2,
  "name": "image utmost",
  "version": "0.0.13",
  "author": "kthjm",
  "description": "image seen in chrome is transformed to utmost (even gif). They can be downloaded by a zip.",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "./icons/16.png",
    "48": "./icons/128.png",
    "128": "./icons/128.png"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "",
  "web_accessible_resources": [],
  "background": {
    "scripts": [
      "./jszip.min.js",
      "./event.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "./icons/16.png",
    "default_title": "image utmost"
  },
  "content_scripts": [{
    "matches": [
      "*://*/*.png*",
      "*://*/*.jpg*",
      "*://*/*.jpeg*",
      "*://*/*.gif*",
      "*://*/*.PNG*",
      "*://*/*.JPG*",
      "*://*/*.JPEG*",
      "*://*/*.GIF*",
      "file://*/*.png",
      "file://*/*.jpg",
      "file://*/*.jpeg",
      "file://*/*.gif",
      "file://*/*.PNG",
      "file://*/*.JPG",
      "file://*/*.JPEG",
      "file://*/*.GIF"
    ],
    "js": [
      "./content.js"
    ],
    "run_at": "document_end"
  }]
}
